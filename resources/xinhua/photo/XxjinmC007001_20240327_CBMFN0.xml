				<ContentItem xsi:type="MultimediaCIType" id="c01">
					<MetaInfo>
						<CharacteristicMetaGroup xsi:type="MultimediaCIMetaGroupType">
							<Format scheme="urn:cnml:xinhua.org:20070701:topiclist.cnml-Format-1:1" topicRef="XHTML" />
